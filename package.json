{
  "name": "koa-template",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development node src/bin/www ",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon bin/www",
    "prd": "cross-env NODE_ENV=production pm2 start src/bin/www",
    "lint": "eslint --ext .js src",
    "lint:fix": "eslint --ext .js src --fix",
    "commit": "npx git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -w -r 0",
    "mysqlConnect": "node ./test/mysqlConnect.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "description": "基于koa封装模板，支持JWT身份认证、文件上传、跨域、异常处理等功能，采用sequelize作为mysql存储ORM框架，支持docker部署",
  "keywords": ["koa", "koa-template", "node", "pm2"],
  "author": "chenzhen <546369005@qq.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/parajs/koa-template.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@koa/cors": "^3.0.0",
    "debug": "^4.1.1",
    "ejs": "~3.0.1",
    "fs-extra": "^8.1.0",
    "http-status-codes": "^1.4.0",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-csrf": "^3.0.8",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.1.0",
    "koa-response-time": "^2.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
    "log4js": "^6.1.0",
    "mysql2": "^2.0.1",
    "nodemailer": "^6.4.16",
    "sequelize": "^5.21.2",
    "sha256": "^0.2.0",
    "shortid": "^2.2.15"
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "conventional-changelog": "^3.1.18",
    "conventional-changelog-cli": "^2.0.31",
    "cross-env": "^6.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.7.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^2.0.1",
    "sequelize-auto": "^0.4.29",
    "standard-version": "^8.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
